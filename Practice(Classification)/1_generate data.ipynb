    "import pandas as pd\n",
    "import random\n",
    "\n",
    "n = int(input())\n",
    "def random_data(n):\n",
    "    data = []\n",
    "    for _ in range(n):\n",
    "        x, y = random.uniform(0, 1), random.uniform(0, 1)\n",
    "        while x == y:\n",
    "            y = random.uniform(0, 1)\n",
    "        data.append((x, y))\n",
    "    return data\n",
    "# 產生數據\n",
    "data = random_data(n)\n",
    "# 轉換成 DataFrame\n",
    "df = pd.DataFrame(data, columns=['x', 'y'])\n",
    "# 設定儲存路徑\n",
    "csv_path = r"data.csv\"放上自己的路徑"\n",
    "# 儲存為 CSV\n",
    "df.to_csv(csv_path, index=False)\n",
    "print(f\"CSV 已儲存至: {csv_path}\")\n",
    "\n",
    "print(\"done\")"
   ]
